{
  "name": "frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  },
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "start:backend": "../venv/bin/python ../manage.py runserver",
    "test:e2e:run": "playwright test",
    "ci:test:e2e": "wait-on -t 30000 http://localhost:5173 http://localhost:8000/api/doctors/ && npm run test:e2e:run",
    "test:e2e": "npm-run-all --parallel --race dev start:backend ci:test:e2e"
  },
  "dependencies": {
    "axios": "^1.11.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "pinia": "^3.0.3",
    "vue": "^3.5.18",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.9.1",
    "jsdom": "^26.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "vite": "^7.0.6",
    "vite-plugin-vue-devtools": "^8.0.0",
    "vitest": "^2.0.5",
    "wait-on": "^8.0.4"
  }
}
